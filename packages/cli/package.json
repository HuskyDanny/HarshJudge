{
  "name": "@harshjudge/cli",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "harshjudge": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "@harshjudge/shared": "workspace:*",
    "@harshjudge/ux": "workspace:*",
    "commander": "^12.1.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "memfs": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
