{
  "name": "@harshjudge/ux",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./server": {
      "types": "./dist-server/index.d.ts",
      "default": "./dist-server/index.js"
    },
    "./server/DashboardServer": {
      "types": "./dist-server/DashboardServer.d.ts",
      "default": "./dist-server/DashboardServer.js"
    }
  },
  "typesVersions": {
    "*": {
      "server": ["./dist-server/index.d.ts"],
      "server/DashboardServer": ["./dist-server/DashboardServer.d.ts"]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && tsc -p tsconfig.server.json",
    "build:server": "tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "serve": "tsc -p tsconfig.server.json && node dist-server/serve.js",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@harshjudge/shared": "workspace:*",
    "chokidar": "^3.6.0",
    "js-yaml": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.18",
    "jsdom": "^24.0.0",
    "memfs": "^4.6.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0",
    "vite": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
